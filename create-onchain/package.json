{
  "name": "create-onchain",
  "description": "Instantly create onchain applications with OnchainKit.",
  "version": "0.0.17",
  "license": "MIT",
  "scripts": {
    "build": "bun run clean && bun run build:esm+types && bun run build:manifest",
    "build:manifest": "cd ../account-manifest && bun install && bun run build && cd ../create-onchain && cp -R ../account-manifest/dist/* ./src/manifest",
    "build:esm+types": "tsc --project tsconfig.build.json --outDir ./dist/esm --declaration --declarationMap --declarationDir ./dist/types",
    "check:types": "tsc --noEmit",
    "clean": "rm -rf dist tsconfig.tsbuildinfo",
    "dev": "bun src/cli.ts",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "files": [
    "dist/**",
    "!dist/**/*.tsbuildinfo",
    "src/**/*.ts",
    "!src/**/*.test.ts",
    "!src/**/*.test-d.ts",
    "templates/**"
  ],
  "bin": "./dist/esm/cli.js",
  "sideEffects": false,
  "type": "module",
  "exports": {
    "./package.json": "./package.json"
  },
  "dependencies": {
    "cac": "^6.7.14",
    "express": "^4.21.2",
    "open": "^10.1.0",
    "ora": "^8.1.0",
    "picocolors": "^1.1.0",
    "prompts": "^2.4.2",
    "ws": "^8.18.1"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/node": "^20.12.10",
    "@types/prompts": "^2.4.9",
    "@types/ws": "^8.5.14",
    "@vitest/coverage-v8": "^3.0.5",
    "@vitest/ui": "^3.0.5",
    "jsdom": "^24.1.0",
    "vitest": "^3.0.5"
  }
}
