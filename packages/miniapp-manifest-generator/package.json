{
  "name": "miniapp-manifest-generator",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "copy": "cp -R ./dist/* ../create-onchain/dist/manifest",
    "build:copy": "pnpm run build && pnpm run copy",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "@coinbase/onchainkit": "latest",
    "react": "^18",
    "react-dom": "^18",
    "wagmi": "^2.14.12",
    "viem": "^2.23.0",
    "buffer": "^6.0.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@tailwindcss/vite": "^4.0.9",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.21.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "tailwindcss": "^4.0.9",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.24.1",
    "vite": "^5.4.16"
  },
  "packageManager": "pnpm@10.6.3"
}
