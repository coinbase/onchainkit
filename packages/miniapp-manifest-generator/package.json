{
  "name": "miniapp-manifest-generator",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:copy": "pnpm run build && node scripts/copyBuild.js",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "@coinbase/onchainkit": "latest",
    "buffer": "^6.0.3",
    "react": "^18",
    "react-dom": "^18",
    "viem": "^2.27.2",
    "wagmi": "^2.14.12"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@tailwindcss/vite": "^4.0.9",
    "@testing-library/jest-dom": "6.4.7",
    "@testing-library/react": "^14.2.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^3.0.5",
    "@vitest/ui": "^3.0.5",
    "eslint": "^9.21.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "jsdom": "^24.1.0",
    "tailwindcss": "^4.0.9",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.24.1",
    "vite": "^6.2.0",
    "vitest": "^3.0.5"
  },
  "packageManager": "pnpm@10.6.3"
}
