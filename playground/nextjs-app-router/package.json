{
  "name": "nextjs-app-router",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "build:clean": "rm -fr ./onchainkit/esm && rm -fr ./onchainkit/src && rm -fr ./node_modules/@coinbase/onchainkit",
    "build:onchainkit": "cd ../.. && yarn install && yarn build",
    "build:link": "cd ./onchainkit && bun link && cd .. && bun link @coinbase/onchainkit",
    "build:prepare": "bun run build:onchainkit && bun run cp && bun i",
    "cp": "cp -R ../../esm ./onchainkit && cp -R ../../src ./onchainkit && cp -R ../../package.json ./onchainkit",
    "cp:dev": "cp -R ../../package.json ./onchainkit/package.json && mkdir -p ./onchainkit/src && touch ./onchainkit/src/styles.css && (test -f ../../src/styles.css && cp -R ../../src/styles.css ./onchainkit/src/styles.css || true)",
    "dev": "next dev",
    "dev:watch": "concurrently \"bun dev\" \"bun run cp:dev && bun build:link && cd ../.. && bun run dev:watch\"",
    "lint": "next lint",
    "start": "next start"
  },
  "dependencies": {
    "@coinbase/onchainkit": "^0.36.4",
    "@radix-ui/react-dropdown-menu": "^2.1.1",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-radio-group": "^1.2.0",
    "@radix-ui/react-select": "^2.1.1",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-switch": "^1.1.0",
    "@radix-ui/react-tabs": "^1.1.0",
    "@reservoir0x/reservoir-sdk": "^2.4.25",
    "@tanstack/react-query": "^5.51.11",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "graphql": "^14 || ^15 || ^16",
    "graphql-request": "^6.1.0",
    "lucide-react": "^0.416.0",
    "next": "^14.2.5",
    "react": "^18",
    "react-dom": "^18",
    "tailwind-merge": "^2.4.0",
    "tailwindcss-animate": "^1.0.7",
    "viem": "^2.21.33",
    "wagmi": "^2.12.24"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "concurrently": "^8.0.0",
    "eslint": "^8",
    "eslint-config-next": "14.2.5",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5",
    "nodemon": "^3.0.0"
  }
}